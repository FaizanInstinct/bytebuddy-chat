{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false",
    "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-1.0.x"
  },
  "regions": ["iad1"]
}